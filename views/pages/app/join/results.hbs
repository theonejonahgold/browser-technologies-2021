<noscript>
  <meta http-equiv="refresh" content="2; url=/app/join?session={{session.code}}&sessid={{sessid}}">
</noscript>
<header>
  {{#if last}}
  <h1>Last question results</h1>
  {{else}}
  <h1>Question results</h1>
  {{/if}}
  <h2>{{question.title}}</h2>
</header>
<main>
  {{#each question.answers}}
  {{> partials/answer-result}}
  {{/each}}
</main>